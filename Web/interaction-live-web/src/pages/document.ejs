<!doctype html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0">
  <title>阿里云电商直播</title>
</head>
<body>
  <script>
    // 简易rem适配
    var width = document.documentElement.clientWidth
    if (width > 750) width = 750
    document.documentElement.style.fontSize = width / 7.5 + 'px';
    window.onresize = () => {
      var width = document.documentElement.clientWidth
      if (width > 750) width = 750
      document.documentElement.style.fontSize = width / 7.5 + 'px';
    };
  </script>
  <div id="root"></div>
