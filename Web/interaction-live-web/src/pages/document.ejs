<!doctype html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0">
  <title>阿里云互动直播</title>
</head>
<body>
  <script>
    var width = document.documentElement.clientWidth
    if (width > 450) width = 450
    document.documentElement.style.fontSize = width / 7.5 + 'px';
    window.onresize = () => {
      var width = document.documentElement.clientWidth
      if (width > 450) width = 450
      document.documentElement.style.fontSize = width / 7.5 + 'px';
    };
  </script>
  <div id="root"></div>
  <script src="https://dev.g.alicdn.com/code/npm/@ali/room-paas-web-engine/0.0.9/RoomPaasInteractionSdk.web.min.js"></script>
</body>
</html>
